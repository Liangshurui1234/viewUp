<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:orientation="vertical"
    android:fitsSystemWindows="true"
    android:id="@+id/container"
    android:layout_height="match_parent"
    tools:context=".ui.main.activity.WebViewActivity">
    <android.support.v7.widget.Toolbar android:layout_height="wrap_content" android:layout_width="match_parent" android:id="@+id/toolBar">

    <TextView android:layout_height="wrap_content" android:layout_width="wrap_content" android:id="@+id/tv_title" android:textSize="@dimen/sp_16" android:textColor="@color/c_333333" android:layout_gravity="center"/>

</android.support.v7.widget.Toolbar>
</LinearLayout>